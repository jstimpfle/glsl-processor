program circle;
program ellipse;

shader projections_vert VERTEX_SHADER "example-shaders/projections.vert";
shader circle_frag FRAGMENT_SHADER "example-shaders/circle.frag";
shader ellipse_frag FRAGMENT_SHADER "example-shaders/ellipse.frag";

link circle projections_vert;
link circle circle_frag;
link ellipse projections_vert;
link ellipse ellipse_frag;
